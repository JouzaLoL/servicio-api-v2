{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "space-before-function-paren": "off",
        "eol-last": "off",
        "no-var": "off",
        "comma-dangle": "off",
        "new-cap": "off",
        "max-len": "off",
        "valid-jsdoc": "off",
        "quotes": "off",
        "no-shadow": "error",
        "object-curly-spacing": "off",
        "no-invalid-this": "off"
    }
}